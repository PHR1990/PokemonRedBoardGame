;   param=value ; assign values to parameters

config_version=4

[application]

config/name="PokemonBoardGame"
run/main_scene="res://Scenes/CardBattleScene/CardBattleScene.tscn"
config/icon="res://icon.png"

[autoload]

Repository="*res://Scripts/Repository.gd"
Pokemon="*res://Scripts/Types/Pokemon.gd"
Enums="*res://Scripts/Types/Enums.gd"
Move="*res://Scripts/Types/Move.gd"
ColorChanger="*res://Scripts/ColorChanger.gd"

[display]

window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
